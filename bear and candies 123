////////////////////////////////////COMMON SENSE/////////////////////////////////////////////
#include<iostream>

using namespace std;
int main()
{
    int t;
    cin>>t;
    int a,b;
    while(t--)
    {
        cin>>a>>b;
        int limak=0,bob=0;
        int n=1;
        while(1)
        {
            limak=limak+n;
            if(limak>a)
            {
                cout<<"Bob"<<endl;
                break;
            }
            n++;
            bob=bob+n;
            if(bob>b)
            {
                cout<<"Limak"<<endl;
                break;
            }
            n++;
        }
    }
}

#include<iostream>
using namespace std;

int main()
{
    int t; //test cases;
    int a,b;
    int mini,maxi;
    cin>>t;
    while(t--)
    {
        cin>>a>>b;
        if(a>=b)
            mini=a;
        else
            mini=b;
        maxi=a+b;
        cout<<mini<<" "<<maxi<<endl;

    }//while
    return 0;
}

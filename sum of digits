#include<iostream>
using namespace std;

int main()
{
    int t; //number of test cases
    int n,sum,k;
    cin>>t;
    while(t--)
    {
        cin>>n;
        sum=0;
        while(n!=0)
        {
            k=n%10;
            sum=sum+k;
            n=n/10;
        }
        cout<<sum<<endl;

    }//while

    return 0;
}//main
